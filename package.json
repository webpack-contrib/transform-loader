{
  "name": "transform-loader",
  "version": "0.2.4",
  "author": "Tobias Koppers @sokra",
  "description": "transform loader for webpack",
  "license": "MIT",
  "main": "dist/cjs.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run build -- -w",
    "build": "cross-env NODE_ENV=production babel src -d dist --ignore 'src/**/*.test.js'",
    "clean": "del-cli dist",
    "clean:dist": "del-cli dist",
    "lint": "eslint --cache src test",
    "lint-staged": "lint-staged",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "release": "standard-version",
    "security": "nsp check",
    "serve:dev": "nodemon $2 --exec babel-node",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverageFrom='src/**/*.js' --coverage",
    "travis:coverage": "npm run test:coverage -- --runInBand",
    "travis:lint": "npm run lint && npm run security",
    "travis:test": "npm run test -- --runInBand",
    "webpack-defaults": "webpack-defaults",
    "appveyor:test": "npm run test"
  },
  "dependencies": {
    "loader-utils": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "brfs": "^1.4.0",
    "coffeeify": "^3.0.0",
    "cross-env": "^5.1.1",
    "del": "^3.0.0",
    "del-cli": "^1.1.0",
    "eslint": "^4.12.1",
    "eslint-config-webpack": "^1.2.5",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "memory-fs": "^0.4.1",
    "nsp": "^3.1.0",
    "pre-commit": "^1.2.2",
    "standard-version": "^4.2.0",
    "webpack": "^3.10.0",
    "webpack-defaults": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webpack-contrib/transform-loader.git"
  },
  "bugs": {
    "url": "https://github.com/webpack-contrib/transform-loader/issues"
  },
  "homepage": "https://github.com/webpack-contrib/transform-loader",
  "engines": {
    "node": ">= 4.8 < 5.0.0 || >= 5.10"
  },
  "peerDependencies": {
    "webpack": "^2.0.0 || ^3.0.0"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
